= content title
  | Index
= content main
  h1 Latest Traces
  hr

  div.row
    div.col-md-12
      table.table.table-bordered
        thead
          tr
            td Time
            td Name
            td Duration
            td Id
        tbody
        {{range .Latest}}
          tr
            td {{Timestamp .Timestamp}}
            td {{.Name}}
            td {{Duration .Duration}}
            td <a href={{URL "trace"}}/{{.TraceId}}>{{.TraceId}}</a>
        {{end}}
